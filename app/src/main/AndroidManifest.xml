<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.asu.bscs.sjmusgro.finalproject" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FuelEntryActivity"
            android:label="@string/title_activity_fuel_entries" >
        </activity>
        <activity
            android:name=".ViewFuelEntriesActivity"
            android:label="@string/title_activity_view_fuel_entries" >
        </activity>
    </application>

</manifest>
